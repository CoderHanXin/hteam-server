  "dependencies": {
    "egg": "^2.4.1",
    "egg-cors": "^2.0.0",
    "egg-scripts": "^2.5.1",
    "egg-sequelize": "^3.1.2",
    "egg-validate": "^1.0.0",
    "blueimp-md5": "^2.10.0",
    "mysql2": "^1.5.2"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.4.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.1",
    "eslint": "^4.18.2",
    "eslint-config-egg": "^7.0.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon",
